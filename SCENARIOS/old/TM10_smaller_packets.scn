####################################################################################
#
#		T-PUT Calculation
#
#####################################################################################
#  No of UEs sending t-put= 1210.1: 15/attach/ + 1126.74/SR/ + 10.12/dedicated/ + 68/Inter-eNB HO/ + 39.87/Intra-eNB HO/ = 1259.73
#  Data distribution is: 25% of packet size 1280 (1252+28); 50% of ping + 25% of heavy packets 1460B (1432+28)
#  DL = 400.0 Mbps /eNB
#  UL = 80.0 Mbps /eNB
#  
####################################################################################
# LTE1819 changes
# SR sessions
# wait 12    --> wait 22
#
# TAU sessions:
# wait 12/15 --> wait 22


transfer TransferDL
    type udp
    packetSize 548
    dir downlink
	#packetRate 58.54			
    packetRate 92.6

transfer TransferUL
	type udp
	packetSize 548
	dir uplink
	#packetRate 11.6
	packetRate 23.6
		
transfer TransferDL_1500
    type udp
    packetSize 1472
    dir downlink
	#packetRate 58.54				
	packetRate 92.6


transfer TransferUL_1500
	type udp
	packetSize 1472
	dir uplink
	#packetRate 11.6
	packetRate 23.6
	
transfer TransferDLPing
	type udp
	packetSize 40
	dir downlink
	packetRate 3.1

transfer TransferULPing
	type udp
	packetSize 40
	dir uplink
	packetRate 3.9

bearerQoS DED
	qci 6
#	mbr_uplink 31
#	mbr_downlink 31
#	gbr_uplink 31
#	gbr_downlink 31
	network_initiated FALSE
	
bearerQoS DED1
	qci 8
#	mbr_uplink 40
#	mbr_downlink 40
#	gbr_uplink 40
#	gbr_downlink 40
	network_initiated FALSE

configMeas conf1

reportMeas report1
        amount      5 
        interval    1
        servingCell rsrpMin=10 rsrpMax=10 rsrqMin=20 rsrqMax=20
        neighCell   rsrpMin=95 rsrpMax=97 rsrqMin=33 rsrqMax=34
        events 2
	skipNeigh	
	
####################################################################################
#
#		IDLE SESSION
#
####################################################################################
#  Total UEs (idle+connected)=2000
#  Total UEs in idle=2000-345(rrc connected)=1655
#
#  This component will require 1504.028 UEs in idle: 1655- 57.642/SR/ - 93.33/TAU/=1504.028
#  to achieve this, the total idle time per UE needs to be 250.67 [ sec ] ( 1504.028 [ UE ] * 1 [ sec ] / 6.0 (attach rate) [ UE ] )  
####################################################################################


application AppIDLE1 [ bearerSR ]
	wait 12
	idleHandover to 101
	wait 100000000000
	idleHandover to 11

mobileSession testSessionIDLE1
	connectBearer DEFAULT as stream1
	startApplication AppIDLE1 [ stream1 ] as myAppSR1
	wait applicationsFinish [ myAppSR1 ]
	releaseBearer stream1	
	
application AppIDLE2 [ bearerSR ]
	wait 12
	idleHandover to 102
	wait 100000000000
	idleHandover to 11

mobileSession testSessionIDLE2
	connectBearer DEFAULT as stream1
	startApplication AppIDLE2 [ stream1 ] as myAppSR1
	wait applicationsFinish [ myAppSR1 ]
	releaseBearer stream1

application AppIDLE3 [ bearerSR ]
	wait 12
	idleHandover to 103
	wait 100000000000
	idleHandover to 11

mobileSession testSessionIDLE3
	connectBearer DEFAULT as stream1
	startApplication AppIDLE3 [ stream1 ] as myAppSR1
	wait applicationsFinish [ myAppSR1 ]
	releaseBearer stream1	
	
####################################################################################
#
#		SERVICE REQUEST
#
####################################################################################
#  1920 RRC Connected UEs
#
#  calculation of AppSR duration:
#  this "component" will require 1759.91 [ UE ] in RRC_CONNECTED ( 1920 - 68 /inter-eNB HO/ - 27.1 /TAU/ - 10.12 /Dedicated/ -15 /ATTACH=PDN -39.87/INTRA-eNB-HO/)
#  to achieve this, the total RRC_CONNECTED time per UE needs to be 139.01 [ sec ] ( 1759.91 [ UE ] * 1 [ sec ] / 12.66 (attach rate) [ UE ] ) :
#  - 10 [ sec ] takes the UE to go idle before detaching
#
#  - Sum of SR: 126.6/2 s; 63.3 = 12.66 attach/s * iterations => iterations = 5
#  - there are 5 iterations of Service Request component, so each iteration needs to have a UE for 27.8 [ sec ] in RRC_CONNECTED ( 139.01 [ sec ] / 5 =~ 27.8)
#  - Inactivity timer = 10s, so 27.8-10[inactivity] = 17.8
#
#  UEs RRC_CONN = 5 [iterations]*(10+17.8)[sec]*12.66 (attach_rate)=1759.74
#  UEs Idle = 12+5*(12-10)[go to idle]*12.66(attach_rate)=161.92
#  T-put UEs = 5 [iteration]*17.8*12.66(attach_rate)=1126.74
###################################################################################
#
# SR sessions
# wait 12 --> wait 22
###################################################################################

application AppSR1 [ bearerSR ]
	wait 22
	idleHandover to 101
	5x
		wait 22
		startTransfer TransferDL on bearerSR as sampleTransferSRDL
		startTransfer TransferUL on bearerSR as sampleTransferSRUL
		wait 17.8
		stopTransfer sampleTransferSRDL
		stopTransfer sampleTransferSRUL
	wait 22
	idleHandover to 11
	
